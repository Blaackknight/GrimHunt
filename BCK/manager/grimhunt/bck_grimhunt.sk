command /shop:
    trigger:
        set {_money} to {grimhunt.%player's uuid%.money}
        set {_runnerMoney} to {grimhunt.runner.money}
        set {_hunterMoney} to {grimhunt.hunter.money}
        if "%{_money}%" is "<none>":
            set {_money} to 0
        if "%{_runnerMoney}%" is "<none>":
            set {_runnerMoney} to 0
        if "%{_hunterMoney}%" is "<none>":
            set {_hunterMoney} to 0
        set {_money} to grimhuntShopAbbrevNumber({_money})
        set {_runnerMoney} to grimhuntShopAbbrevNumber({_runnerMoney})
        set {_hunterMoney} to grimhuntShopAbbrevNumber({_hunterMoney})
        
        if {bck.runners::*} contain player:
            add "a" to {_args::*}
            add "%{_money}%" to {_args::*}
            add "%{_runnerMoney}%" to {_args::*}
        else if {bck.hunters::*} contain player:
            add "4" to {_args::*}
            add "%{_money}%" to {_args::*}
            add "%{_hunterMoney}%" to {_args::*}
        else:
            add "1" to {_args::*}
            add "%{_money}%" to {_args::*}
            add "0" to {_args::*}
        
        set {_n} to superLang("grimhunt.command.shop.gui.title", {_player}, {_args::*})
        #broadcast "%{_money}% = %{_n}%"
        set {_gui} to genericGui({_n}, 5, true)
        set slot 22 of {_gui} to redstone block named "§4Wait.."
        userData(player, "data.inv", "set", "manager.grimhunt.shop.main")
        open {_gui} to player
        grimhuntShopGuiUpdater(player)
#                                       grimhuntShopGuiUpdater functions                                                                         #
function grimhuntShopGuiUpdater(player: player):
    set {_gui} to {_player}'s current inventory
    set {_money} to {grimhunt.%{_player}'s uuid%.money}
    set {_runnerMoney} to {grimhunt.runner.money}
    set {_hunterMoney} to {grimhunt.hunter.money}
    if "%{_money}%" is "<none>":
        set {_money} to 0
    if "%{_runnerMoney}%" is "<none>":
        set {_runnerMoney} to 0
    if "%{_hunterMoney}%" is "<none>":
        set {_hunterMoney} to 0
    set {_money} to grimhuntShopAbbrevNumber({_money})
    set {_runnerMoney} to grimhuntShopAbbrevNumber({_runnerMoney})
    set {_hunterMoney} to grimhuntShopAbbrevNumber({_hunterMoney})
    if {bck.runners::*} contain player:
        add "a" to {_args::*}
        add "%{_money}%" to {_args::*}
        add "%{_runnerMoney}%" to {_args::*}
    else if {bck.hunters::*} contain player:
        add "4" to {_args::*}
        add "%{_money}%" to {_args::*}
        add "%{_hunterMoney}%" to {_args::*}
    else:
        add "1" to {_args::*}
        add "%{_money}%" to {_args::*}
        add "0" to {_args::*}
    set slot 22 of {_gui} to air
    set {_gui}'s name to superLang("grimhunt.command.shop.gui.title", {_player}, {_args::*})
    loop yaml node keys "shop.choices" from "bck.manager.grimhunt.data":
        add loop-node to {_shopChoices::*}
    set {_o::*} to all items
    set {_sc} to 10
    set {_gui} to {_player}'s current inventory
    set {_gc} to 0
    loop 53 times:
        if {_gc} is 10 or 19 or 28:
            add 7 to {_gc}
        else if {_gc} is 40:
            add 1 to {_gc}
        set slot {_gc} of {_gui} to black stained glass pane named "§l"
        add 1 to {_gc}
    if listCounter({_shopChoices::*}) > 21:
        set {_ti} to yaml value "grimhunt.command.shop.gui.nextPage.item" from "bck.manager.grimhunt.data"
        set {_ti} to {_ti} parsed as item
        set slot 44 of {_gui} to {_ti} named lang("grimhunt.command.shop.gui.nextPage.name", {_player})
    loop {_shopChoices::*}:
        if {_sc} is 17 or 26:
            add 2 to {_sc}
        set {_x} to loop-value
        set {_tt::*} to {_x} split by "."
        set {_r} to {_tt::3}
        replace all "_" with " " in {_r}
        if "%{_o::*}%" contain "%{_r}%":
            set {_size} to yaml value "%{_x}%.size" from "bck.manager.grimhunt.data"
            set {_cost} to yaml value "%{_x}%.cost" from "bck.manager.grimhunt.data"
            set {_i} to {_r} parsed as item
            add "%{_cost}%" to {_arg::*}
            add noLang("grimhunt.shop.moneySymbol") to {_arg::*}
            set line 2 of lore of {_i} to superLang("grimhunt.command.shop.gui.cost", {_player}, {_arg::*})
            set line 4 of lore of {_i} to lang("grimhunt.command.shop.gui.youBuy", {_player})
            set line 5 of lore of {_i} to lang("grimhunt.command.shop.gui.teamBuy", {_player})
            set slot {_sc} of {_gui} to {_size} of {_i}
            #broadcast "##%{_sc}% = %{_i}%"
            clear {_arg::*}
        else:
            set {_size} to yaml value "%{_x}%.size" from "bck.manager.grimhunt.data"
            set {_cost} to yaml value "%{_x}%.cost" from "bck.manager.grimhunt.data"
            set {_i} to barrier named "§4§l§m§o%{_r}%"
            add "%{_cost}%" to {_arg::*}
            add noLang("grimhunt.shop.moneySymbol") to {_arg::*}
            set line 2 of lore of {_i} to superLang("grimhunt.command.shop.gui.cost", {_player}, {_arg::*})
            set slot {_sc} of {_gui} to {_i}
            clear {_arg::*}
        add 1 to {_sc}
    userData({_player}, "data.inv", "set", "manager.grimhunt.shop.main")
