                    set {_data::*} to {_m} split by "_money"
                    set {_mob} to {_data::1}
                    replace "_" with " " in {_mob}
                    set {_moo} to {_mob}
                    if {_moo} is "%{_t}%":
                        grimhuntAddShopMoney(player, {_v})
                    #set {_shopOre.%{_sb}%} to yaml value "%loop-value%" from "bck.manager.grimhunt.data"
                    add 1 to {_sb}
