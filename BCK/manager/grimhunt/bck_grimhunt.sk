function grimhuntShopAbbrevNumber(v: integer) :: text:
    if {_v} >= 1000:
        if {_v} < 1000000:
            set {_v} to "%{_v} / 1000%k"
        else:
            set {_v} to "%{_v} / 1000000%m"
    else:
        set {_v} to "%{_v}%"
    return {_v}
function grimhuntGetValidShopChoices(player: player, item: boolean=false) :: objects:
    loop yaml node keys "shop.choices" from "bck.server.grimhunt.config":
        add loop-node to {_shopChoices::*}
    set {_o::*} to all items
    set {_sc} to 0
    loop {_shopChoices::*}:
        set {_x} to loop-value
        set {_tt::*} to {_x} split by "."
        set {_r} to {_tt::3}
        
        set {_size} to yaml value "%{_x}%.size" from "bck.server.grimhunt.config"
        set {_size} to {_size} parsed as integer
        set {_cost} to yaml value "%{_x}%.cost" from "bck.server.grimhunt.config"
        set {_cost} to {_cost} parsed as integer
        if "%{_o::*}%" contain "%{_r}%":
            if {_item} is false:
                set {_syb} to noLang("moneySymbol")
                add "§e§l⇾ §r§b%{_r}%§dx%{_size}%§e/§a%{_cost}% %{_syb}%" to {_r::*}
            else:
                set {_i} to {_r} parsed as item
                add "%{_cost}%" to {_args::*}
                set line 2 of lore of {_i} to superLang("grimhunt.command.shop.gui.cost", {_player}, {_args::*})
                add {_i} to {_r::*}
                #broadcast "%{_r::*}%"
        else:
            if {_item} is false:
                set {_syb} to noLang("moneySymbol")
                add "§e§l⇾ §r§3§m%{_r}%§r§dx%{_size}%§e/§a%{_cost}% %{_syb}%" to {_r::*}
            else:
                set {_i} to barrier named "§4§l??"
                add {_i} to {_r::*}
    return {_r::*}
function grimhuntAddShopMoney(player: player, amount: integer=1):
    add {_amount} to {grimhunt.%{_player}'s uuid%.money}
    set {_s} to noLang("grimhunt.shop.moneySymbol")
    set {_se} to selector({_player})
    superLog("§6[§4BCK§6/§5Grimhunt§6]§r §7§l%{_amount}% %{_s}%§r has been added to §7%{_se}%§r", "6.4")
function grimhuntRemoveShopMoney(player: player, amount: integer=1):
    remove {_amount} from {grimhunt.%{_player}'s uuid%.money}
    set {_s} to noLang("grimhunt.shop.moneySymbol")
    set {_se} to selector({_player})
    superLog("§6[§4BCK§6/§5Grimhunt§6]§r §7§l%{_amount}% %{_s}%§r has been removed from §7%{_se}%§r", "6.4")
function grimhuntSetShopMoney(player: player, amount: integer=1):
    set {grimhunt.%{_player}'s uuid%.money} to {_amount} 
    set {_s} to noLang("grimhunt.shop.moneySymbol")
    set {_se} to selector({_player})
    superLog("§6[§4BCK§6/§5Grimhunt§6]§r §7§l%{_amount}% %{_s}%§r has been set to §7%{_se}%§r", "6.4")
function grimhuntAddShopTeamMoney(player: player, amount: integer=1):
    set {_s} to noLang("grimhunt.shop.moneySymbol")
    if {bck.runners::*} contain {_player}:
        add {_amount} to {grimhunt.runner.money}
        superLog("§6[§4BCK§6/§5Grimhunt§6]§r §7§l%{_amount}% %{_s}%§r has been added to §7Runners§r", "6.4")
    else if {bck.hunters::*} contain {_player}:
        add {_amount} to {grimhunt.hunter.money}
        superLog("§6[§4BCK§6/§5Grimhunt§6]§r §7§l%{_amount}% %{_s}%§r has been added to §7Hunters§r", "6.4")
function grimhuntRemoveShopTeamMoney(player: player, amount: integer=1):
    set {_s} to noLang("grimhunt.shop.moneySymbol")
    if {bck.runners::*} contain {_player}:
        remove {_amount} from {grimhunt.runner.money}
        superLog("§6[§4BCK§6/§5Grimhunt§6]§r §7§l%{_amount}% %{_s}%§r has been removed from §7Runners§r", "6.4")
    else if {bck.hunters::*} contain {_player}:
        remove {_amount} from {grimhunt.hunter.money}
        superLog("§6[§4BCK§6/§5Grimhunt§6]§r §7§l%{_amount}% %{_s}%§r has been removed from §7Hunters§r", "6.4")
function grimhuntGetRandomMoney(min: integer=1, max: integer=10) :: integer:
    return rng({_min}, {_max})
on death:
    set {_a} to attacker
    set {_v} to victim
    if "%type of {_v}%" is "player":
        if grimhuntDataBoolean("shop.status") is true:
            if {grimhunt.game.started} is true:
                if {bck.hunters::*} contain {_v}:
                    set {_xM} to {grimhunt.%{_v}'s uuid%.money}
                    set {_death} to grimhuntData("shop.deathMoneyLoose")
                    set {_death} to {_death} parsed as number
                    if {_death} is not set:
                        set {_death} to 0.9
                    set {_xMA} to {_xM} * {_death}
                    grimhuntSetShopMoney({_v}, {_xMA})
                    if {bck.runners::*} contain {_a}:
                        set {_count} to grimhuntGetRandomMoney(1, 1000)
                        grimhuntAddShopMoney({_a}, {_count})
on break:
    set {_b} to event-block
    set {_t} to type of {_b}
    if grimhuntDataBoolean("shop.status") is true:
